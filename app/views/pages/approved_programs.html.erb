<h3>All Programs</h3>
<%= form_tag(pages_approved_programs_path, :method => "get", id: "search-form") do %>
  <%= text_field_tag :search, params[:search], placeholder: "Search State or Suburb or Postcode"%>
  <%= submit_tag "Search" %>
<% end %>
<div class="row">
  <div class="col s6">
    <% if @programs.present? %>
      <%= render @programs %>
    <% else %>
      <p>There are no listings located in <%= params[:search] %>.</p>
    <% end %>
  </div>
</div>
