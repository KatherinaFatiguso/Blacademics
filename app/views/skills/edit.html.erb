<% if user_signed_in? %>
  <% if current_user.id == @skill.student_profile.user.id %>
    <div class="row">
      <div class="col s12 m8 offset-m2 l8 offset-l2">
        <p>
          <h4>Editing Skill</h4>
        </p>
        <%= render 'form' %>
      </div>
    </div>
  <% else %>
    <%= render "listings/forbidden" %>
  <% end %>
<% end %>
