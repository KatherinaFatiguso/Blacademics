<% if user_signed_in? %>
  <% if current_user.id == @student_profile.user.id %>
    <div class="row">
      <div class="col s12 m8 offset-m2 l8 offset-l2">
        <h4>Editing Student Profile</h4>
        current_user.id <%= current_user.id %> <br> @student_profile.user.id <%= @student_profile.user.id %>
        <%= render 'form' %>
        <%= link_to 'Show', @student_profile %>
      </div>
    </div>
  <% else %>
    <%= render "listings/forbidden" %>
  <% end %>
<% end %>
